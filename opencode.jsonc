{
  "$schema": "https://opencode.ai/config.json",
  "plugin": ["opencode-antigravity-auth@beta"],
  "agent": {
    "Clopus the Orchestrator": {
      "description": "Strategic intelligence layer - plans, delegates, and ensures quality. Does not implement.",
      "mode": "primary",
      "model": "anthropic/claude-opus-4-5",
      "thinking": {
        "type": "enabled",
        "budgetTokens": 32000
      },
      "prompt": "{file:./prompts/orchestrator.txt}"
    },
    "Clopus the Orchestrator (Antigravity)": {
      "description": "Strategic intelligence layer - plans, delegates, and ensures quality. Does not implement.",
      "mode": "primary",
      "model": "google/antigravity-claude-opus-4-5-thinking",
      "variant": "max",
      "prompt": "{file:./prompts/orchestrator.txt}"
    },
    "G-3PO the Orchestrator": {
      "description": "Strategic intelligence layer - plans, delegates, and ensures quality. Does not implement.",
      "mode": "primary",
      "temperature": 1.0,
      "model": "google/gemini-3-pro-preview",
      "thinkingConfig": {
        "thinkingLevel": "high",
        "includeThoughts": true
      },
      "prompt": "{file:./prompts/orchestrator.txt}"
    },
    "Codex the Orchestrator": {
      "description": "Strategic intelligence layer - plans, delegates, and ensures quality. Does not implement.",
      "mode": "primary",
      "temperature": 0.1,
      "model": "openai/gpt-5.2-codex",
      "reasoningEffort": "high",
      "reasoningSummary": "auto",
      "prompt": "{file:./prompts/orchestrator.txt}"
    },
    "minimax-coder": {
      "description": "Code implementation specialist with full tool access (Minimax)",
      "mode": "subagent",
      "model": "opencode/minimax-m2.1-free",
      "prompt": "{file:./prompts/executor.txt}"
    },
    "minimax-verifier": {
      "description": "Code reviewer and quality checker - read-only analysis (Minimax)",
      "mode": "subagent",
      "model": "opencode/minimax-m2.1-free",
      "tools": {
        "write": false,
        "edit": false
      },
      "prompt": "{file:./prompts/verifier.txt}"
    },
    "gemini-coder": {
      "description": "Code implementation specialist with full tool access (Gemini)",
      "mode": "subagent",
      "temperature": 1.0,
      "model": "google/gemini-3-flash-preview",
      "thinkingConfig": {
        "thinkingLevel": "high",
        "includeThoughts": true
      },
      "prompt": "{file:./prompts/executor.txt}"
    },
    "gemini-verifier": {
      "description": "Code reviewer and quality checker - read-only analysis (Gemini)",
      "mode": "subagent",
      "temperature": 1.0,
      "model": "google/gemini-3-flash-preview",
      "thinkingConfig": {
        "thinkingLevel": "high",
        "includeThoughts": true
      },
      "tools": {
        "write": false,
        "edit": false
      },
      "prompt": "{file:./prompts/verifier.txt}"
    },
    "elder-verifier": {
      "description": "Final verification with gpt-5.2-codex. SLOW (15-30 min). Finds the most elusive bugs.",
      "mode": "subagent",
      "model": "openai/gpt-5.2-codex",
      "reasoningEffort": "xhigh",
      "reasoningSummary": "auto",
      "temperature": 0.1,
      "tools": {
        "write": false,
        "edit": false
      },
      "prompt": "{file:./prompts/tortoise-verifier.txt}"
    },
    "tortoise-verifier": {
      "description": "Final verification with gpt-5.2-codex. SLOW (10-20 min). Wins the race, eventually.",
      "mode": "subagent",
      "model": "openai/gpt-5.2-codex",
      "reasoningEffort": "high",
      "reasoningSummary": "auto",
      "temperature": 0.1,
      "tools": {
        "write": false,
        "edit": false
      },
      "prompt": "{file:./prompts/tortoise-verifier.txt}"
    },
    "tortoise-coder": {
      "description": "Implementation specialist with gpt-5.2-codex. SLOW (10-20 min). For difficult tasks where faster agents have failed.",
      "mode": "subagent",
      "model": "openai/gpt-5.2-codex",
      "reasoningEffort": "high",
      "reasoningSummary": "auto",
      "temperature": 0.1,
      "prompt": "{file:./prompts/tortoise-coder.txt}"
    },
    "ui wizard": {
